from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver import ActionChains
import time
import pyautogui
import pandas as pd

# Registrar dados do usuário
user = input("Digite o seu login do GAL:")
senha = input("Digite a sua senha do GAL:")
CNES = input("Digite o seu número de CNS (profissional solicitante):")


# Abrir navegador
options = webdriver.ChromeOptions()
options.add_argument('--ignore-ssl-errors=yes')
options.add_argument('--ignore-certificate-errors')
options.add_argument("--start-maximized")
driver = webdriver.Chrome(options=options)
driver.get("https://gal.distritofederal.sus.gov.br/")

#Login
driver.find_element(By.CSS_SELECTOR, "#ext-comp-1008").send_keys(user)
driver.find_element(By.NAME, "senha").send_keys(senha)
driver.find_element(By.NAME, "modulo").send_keys("BIOLOGIA")
time.sleep(2)
driver.find_element(By.XPATH, "/html/body/div[9]/div[1]/div").click()
time.sleep(1)
driver.find_element(By.ID, "ext-comp-1011").send_keys("LACEN-DF")
time.sleep(1)
driver.find_element(By.CSS_SELECTOR, "#ext-gen202 > div:nth-child(1)").click()
time.sleep(1)
driver.find_element(By.CSS_SELECTOR, "#ext-gen68").click()
time.sleep(7)
driver.find_element(By.CSS_SELECTOR, "#ext-gen308").click()
time.sleep(2)
pyautogui.press('esc')
time.sleep(2)

#Entrar na aba de registro
action = ActionChains(driver)
driver.find_element(By.CSS_SELECTOR, ".x-tree-root-node > li:nth-child(2) > div:nth-child(1) > img:nth-child(2)").click()
time.sleep(2)
entrada = driver.find_element(By.XPATH, "/html/body/div[4]/div[2]/div/div/div/div/ul/div/li[2]/ul/li[1]/div/a/span")
action.double_click(on_element=entrada)
action.perform()
time.sleep(2)
driver.find_element(By.XPATH, "/html/body/div[4]/div[2]/div/div/div/div/ul/div/li[2]/ul/li[1]/ul/li[1]/div/a/span").click()
time.sleep(6)

tabela = pd.read_csv("tabela.csv")
print(tabela)

# Começar o cadastro...
for i, solicitacao in enumerate(tabela["solicitacao"]):
    finalidade = tabela.loc[i, "finalidade"]
    descricao = tabela.loc[i, "descricao"]
    CNS = tabela.loc[i, "CNS"]
    motivo = tabela.loc[i, "motivo"]
    tratamento = tabela.loc[i, "tratamento"]
    populacao = tabela.loc[i, "populacao"]
    contato = tabela.loc[i, "contato"]
    amostra = tabela.loc[i, "amostra"]
    quantidade = tabela.loc[i, "quantidade"]
    pesquisa = tabela.loc[i, "pesquisa"]

# Mudar para aba de cadastro
    time.sleep(2)
    pyautogui.hotkey('ctrl', 'f')
    time.sleep(2)
    pyautogui.write('incluir')
    time.sleep(2)
    pyautogui.press('enter')
    time.sleep(2)
    pyautogui.press('esc')
    time.sleep(2)
    pyautogui.press('enter')
    time.sleep(2)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.write('0011371')
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(3)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.write(CNES)
    pyautogui.press("tab")
    time.sleep(7)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press('tab')
    time.sleep(1)
    pyautogui.write(solicitacao)
    time.sleep(2)
    pyautogui.press("tab")
    pyautogui.write(str(CNS))
    time.sleep(3)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.write(descricao)
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.write(motivo)
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.write(tratamento)
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.write(populacao)
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.write(contato)
    time.sleep(2)
    pyautogui.press("tab", presses = 1)
    time.sleep(2)
    ## Cadastro de amostra
    pyautogui.write(amostra)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.write(str(quantidade))
    time.sleep(2)
    pyautogui.press("tab")
    time.sleep(1)
    pyautogui.press("tab")
    time.sleep(2)
    pyautogui.write(solicitacao)
    time.sleep(2)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.press('tab')
    time.sleep(1)
    pyautogui.press('tab')
    time.sleep(1)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.press('enter')
    time.sleep(2)
    # Pesquisa/Exames
    pyautogui.write(pesquisa)
    time.sleep(2)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.press('down')
    pyautogui.press('enter')
    time.sleep(2)
    pyautogui.press('tab')
    time.sleep(2)
    pyautogui.press('enter')
    time.sleep(3)
    pyautogui.hotkey('ctrl', 'f')
    time.sleep(2)
    pyautogui.write('salvar')
    time.sleep(2)
    pyautogui.press('enter')
    time.sleep(2)
    pyautogui.press('esc')
    time.sleep(2)
    pyautogui.press('enter')
    time.sleep(5)
    pyautogui.press('esc')
    time.sleep(8)
